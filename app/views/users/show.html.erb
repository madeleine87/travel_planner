<h1>User</h1>
<div>
	id:
	<%= @user.id %>
</div>
<div>
	Username:
	<%= @user.username %>
</div>
<div>
	Email:
	<%= @user.email %>
</div>
<div>
	First name:
	<%= @user.first_name %>
</div>
<div>
	Last name:
	<%= @user.last_name %>
</div>
<div>
	City:
	<%= @user.city.name unless @user.city == nil %>
</div>
<% if can? :read, User %>
	<%= link_to "Back", users_path %>
<% end %>
