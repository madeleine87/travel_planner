<h1>Countries</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th>id</th>
			<th>Country</th>
			<th>Edit</th>
			<th>Delete</th>
		</tr>
	</thead>
	<tbody>
		<% @countries.each do |country| %>
		<tr>
			<td><%= country.id %></td>
			<td><%= country.name %></td>
			<td><%= link_to "Edit", edit_country_path(country) %></td>
			<td><%= link_to "Delete", country, method: :delete %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<p><%= link_to "New", new_country_path %></p>